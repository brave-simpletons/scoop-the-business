    },
    "architecture": {
        "64bit": {
            "url": "https://slproweb.com/download/Win64OpenSSL-1_1_1m.exe",
            "hash": "d40be1f440e8083527577fd2dc5d5966045072a02ae20872a5727036a41549ac"
        },
        "32bit": {
            "url": "https://slproweb.com/download/Win32OpenSSL-1_1_1m.exe",
